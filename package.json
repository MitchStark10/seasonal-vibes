{
  "name": "exampleazureapp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev:client": "cd client && npm run start",
    "dev": "concurrently --kill-others \"cross-env PORT=3001 npm run dev:client\" \"npm run start\"",
    "start": "node app",
    "build": "cd client && npm i && npm run build",
    "postinstall": "cd client && npm i"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "prettier": "^3.0.0"
  }
}
