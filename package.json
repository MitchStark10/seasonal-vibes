{
  "name": "scheduled-vibes",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:client": "cd client && npm run start",
    "dev": "concurrently --kill-others \"cross-env PORT=3001 REACT_APP_LOGIN_URL=http://localhost:3000/auth/login SETTINGS_API_URI=http://localhost:3000/settings npm run dev:client\" \"npm run start\"",
    "start": "nodemon app",
    "build": "cd client && npm i && npm run build",
    "postinstall": "cd client && npm i",
    "generatePlaylists": "node ./cron/createScheduledPlaylist.js"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "got": "^13.0.0",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "mssql": "^9.1.3"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0"
  }
}
